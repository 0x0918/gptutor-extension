{
  "name": "gptutor",
  "displayName": "GPTutor (ðŸ¤–â€šðŸ¤–)",
  "repository": {
    "type": "git",
    "url": "https://github.com/RayHuang880301/gptutor-extension"
  },
  "description": "A VS Code extension for smart contract development.",
  "publisher": "gptutor",
  "version": "0.0.103",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "out/resources/extensionIcon.png",
  "activationEvents": [
    "onView:gptutor.chatView"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Initialize GPTutor",
        "title": "GPTutor: init"
      },
      {
        "command": "Set OpenAI API Key",
        "title": "GPTutor: setKey"
      },
      {
        "command": "Delete OpenAI API Key",
        "title": "GPTutor: deleteKey"
      },
      {
        "command": "Set Model",
        "title": "GPTutor: setModel"
      },
      {
        "command": "GPTutor Active",
        "title": "GPTutor: active"
      },
      {
        "command": "GPTutor Explain",
        "title": "GPTutor: explain"
      },
      {
        "command": "Audit GPTutor",
        "title": "GPTutor: audit"
      },
      {
        "command": "GPTutor Comment",
        "title": "GPTutor: comment"
      },
      {
        "command": "GPTutor Edit Prompts",
        "title": "GPTutor: edit prompts"
      },
      {
        "command": "GPTutor Reset Prompts Config",
        "title": "GPTutor: reset prompts"
      }
    ],
    "menus": {
      "editor/context": [],
      "commandPalette": []
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gptutor",
          "title": "GPTutor",
          "icon": "out/resources/icon.png"
        }
      ]
    },
    "views": {
      "gptutor": [
        {
          "type": "webview",
          "id": "gptutor.chatView",
          "name": "GPTutor"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && copyfiles -u 1 \"src/resources/**/*\" out && copyfiles -u 1 \"src/media/**/*\" out && rm out/media/prompt_config.json",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "npm run compile && vsce package && vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "copyfiles": "^2.4.1",
    "openai": "^3.2.1"
  }
}
