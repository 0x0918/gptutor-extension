{
  "name": "gptutor",
  "displayName": "GPTutor (ðŸ¤–â€šðŸ¤–)",
  "description": "A VS Code extension for smart contract development.",
  "publisher": "gptutor",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "out/resources/extensionIcon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Initialize GPTutor",
        "title": "GPTutor: init"
      },
      {
        "command": "Set OpenAI API Key",
        "title": "GPTutor: setKey"
      },
      {
        "command": "Set Model",
        "title": "GPTutor: setModel"
      },
      {
        "command": "Active GPTutor",
        "title": "GPTutor: active"
      },
      {
        "command": "Audit GPTutor",
        "title": "GPTutor: audit"
      }
    ],
    "menus": {
      "editor/context": [

      ],
      "commandPalette": [
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gptutor",
          "title": "GPTutor",
          "icon": "src/resources/icon.png"
        }
      ]
    },
    "views": {
      "gptutor": [
        {
          "type": "webview",
          "id": "gptutor.chatView",
          "name": "GPTutor"
        }
      ]
    }
  },
  "activationEvents": [
    "onView:gptutor.chatView"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
